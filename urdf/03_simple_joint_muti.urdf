<?xml version="1.0"?>
<robot name="visual">
    
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1"/>
    </material>
    <material name="black">
        <color rgba="0.0 0.0 0 1"/>
    </material>
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.2" length="0.6"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <link name="right_leg">
        <visual>
            <origin rpy="0 1.57075 0" xyz="0 0 -0.3"/>
            <geometry>
                <box size="0.6 0.1 0.2"/>
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <joint name="base_to_right_leg" type="fixed">
        <parent link="base_link"/>
        <child link="right_leg"/>
        <origin xyz="0 -0.22 0.25"/>
    </joint>

    <link name="right_base">
        <visual>
            <geometry>
                <box size="0.4 0.1 0.1"/>
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <joint name="right_base_joint" type="fixed">
        <parent link="right_leg"/>
        <child link="right_base"/>
        <origin xyz="0 0.0 -0.6"/>
    </joint>

    <link name="right_front_wheel">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.57075 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.035" length="0.1"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <joint name="right_front_wheel_joint" type="continuous">
        <axis xyz="0.0 1 0.0" rpy ="0 0 0 "/>
        <parent link="right_base"/>
        <child link="right_front_wheel"/>
        <origin xyz="0.1333333333 0.0 -0.085" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="right_back_wheel">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.57075 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.035" length="0.1"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <joint name="right_back_wheel_joint" type="continuous">
        <axis xyz="0.0 1.0 0.0" rpy="0 0 0"/>
        <parent link="right_base"/>
        <child link="right_back_wheel"/>
        <origin xyz="-0.13333333333 0.0 -0.085" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="left_leg">
        <visual>
            <geometry>
               <box size="0.6 0.1 0.2"/> 
            </geometry>
            <origin xyz="0.0 0.0 -0.3" rpy="0.0 1.57075 0.0"/>
            <material name="white"/>
        </visual>
    </link>

    <joint name="base_to_left_leg" type="fixed">
        <parent link="base_link"/>
        <child link="left_leg"/>
        <origin xyz="0.0 0.22 0.25"/>
    </joint>

    <link name="left_base">
        <visual>
            <geometry>
                <box size="0.4 0.1 0.1"/>
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <joint name="left_base_joint" type="fixed">
        <parent link="left_leg"/>
        <child link="left_base"/>
        <origin xyz="0.0 0.0 -0.6"/>
    </joint>

    <link name="left_front_wheel">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.57075 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.035" length="0.1"/>
            </geometry>    
            <material name="black"/>
        </visual>
    </link>

    <joint name="left_front_wheel_joint" type="continuous">
        <axis xyz="0.0 1.0 0.0" rpy="0 0 0"/>
        <parent link="left_base"/>
        <child link="left_front_wheel"/>
        <origin xyz="0.133333333333333 0.0 -0.085" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="left_back_wheel">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.57075 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.035" length="0.1"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <joint name="left_back_wheel_joint" type="continuous">
        <axis xyz="0.0 1.0 0.0" rpy="0 0 0"/>
        <parent link="left_base"/>
        <child link="left_back_wheel"/>
        <origin xyz="-0.1333333333333 0.0 -0.085" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="gripper_extension" type="prismatic">
        <parent link="base_link"/>
        <child link="gripper_pole"/>
        <limit lower="-0.38" upper="0.0" effort="1000.0" velocity="20.0"/>
        <origin xyz="0.19 0.0 0.2" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="gripper_pole">
        <visual>
            <origin xyz="0.1 0.0 0.0" rpy="0.0 1.57075 0.0"/>
            <geometry>
                <cylinder radius="0.01" length="0.2"/>
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <joint name="left_gripper_joint" type="revolute">
        <axis xyz="0.0 0.0 1"/>
        <limit lower="0.0" upper="0.548" effort="1000.0" velocity="20.0"/>
        <origin xyz="0.2 0.01 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="gripper_pole"/>
        <child link="left_gripper"/>
    </joint>

    <link name="left_gripper">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://urdf_tutorial/meshes/l_finger.dae"/>
            </geometry>
        </visual>
    </link>

    <joint name="left_tip_joint" type="fixed">
        <parent link="left_gripper"/>
        <child link="left_tip"/>
    </joint>

    <link name="left_tip">
        <visual>
            <origin xyz="0.09137 0.00495 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://urdf_tutorial/meshes/l_finger.dae"/>
            </geometry>
        </visual>
    </link>
</robot>
